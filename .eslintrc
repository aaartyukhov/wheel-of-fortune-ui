{
  "extends": [
    "airbnb-base",
    "plugin:react/recommended",
    "plugin:import/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": "error",
    "no-underscore-dangle": ["error", { "allow": ["_id"]}],
  },
  "env": {
    "browser": true
  }
}
